Proceso Ejercicio18
	//Pedir un número e indicar si
	//es positivo o negativo.
	
	//DECLARACIÓN/ DEFINICIÓN 
	Definir num1 como entero;
	//Entrada 
	Escribir "Digite el número";
	Leer num1;
	//PROCESO - SALIDA
	si (num1>0) entonces 
		Escribir "El numero es positivo";
	SiNo
		Escribir "Escribir el numero es negativo";
	FinSi
	
	
FinProceso
