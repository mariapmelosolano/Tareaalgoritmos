Proceso Ejemplo05
	//DECLARACION/DEFINICION VARIABLES
	Definir num1,num2,resultado como real;
	//ENTRADA DATOS
	Escribir "Digite el valor del numero1";
	Leer num1;
	Escribir "Digite el valor del numero 2";
	Leer num2;
	//PROCESOS-OPERACIONES-FORMULAS 
	resultado<-num1+num2;

	//SALIDA DATOS 
	Escribir "El resultado de sumar", num1, "+", num2, "es:", resultado;
	
FinProceso
